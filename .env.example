DATABASE_URL="mysql://db_user:db_password@mysql:3306/db_name?serverVersion=8.0"

DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=db_name
DB_USERNAME=db_user
DB_PASSWORD=db_password
DB_ROOT_PASSWORD=db_password
FORWARD_DB_PORT=3306
WWWGROUP=www-data

###> symfony/framework-bundle ###
APP_ENV=dev
APP_SECRET=
